<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="rmjsoft.scrollviewapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MyTextViewActivity" />
        <activity android:name=".ImageViewActivity" />
        <activity android:name=".EditTextActivity" />
        <activity android:name=".MyAutoCompleteTextViewActivity" />
        <activity android:name=".ButtonActivity" />
        <activity android:name=".ImageButton" />
        <activity android:name=".ImageButtonActivity" />
        <activity android:name=".CheckBoxActivity" />
        <activity android:name=".ToggleButtonActivity" />
        <activity android:name=".MyRadioButtonActivity" />
        <activity android:name=".MyRadioGroupActivity" />
        <activity android:name=".MyPogressBarActivity" />
        <activity android:name=".MySpinnerActivity" />
        <activity android:name=".MyTimePickerActivity" />
        <activity android:name=".MyDatePickerActivity"></activity>
    </application>

</manifest>